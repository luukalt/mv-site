{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "rewrites": [
      {
        "source": "/contents/:splat",
        "destination": "https://firebasestorage.googleapis.com/v0/b/marieke-versleijen.appspot.com/o/contents%2F:splat?alt=media"
      }
    ]
  }
}