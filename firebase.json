
{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "rewrites": [
      {
        "source": "/contents/**",
        "destination": "https://firebasestorage.googleapis.com/v0/b/[your-bucket-name].appspot.com/o/contents%2F"
      }
    ]
  }
}