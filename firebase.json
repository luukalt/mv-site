{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "rewrites": [
      {
        "source": "/contents/**",
        "destination": "https://firebasestorage.googleapis.com/v0/b/marieke-versleijen.appspot.com/o/contents/**?alt=media"
      }
    ],
    "headers": [
      {
        "source": "/contents/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=2592000, s-maxage=2592000"
          }
        ]
      }
    ]
  }
}
